<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\UsersLastLogin" table="users_last_login">
    <indexes>
      <index name="kom" columns="kom"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastLogin" type="datetime" column="last_login" nullable="false"/>
    <many-to-one field="kom" target-entity="Cars" fetch="LAZY">
      <join-columns>
        <join-column name="kom" referenced-column-name="Kom"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
